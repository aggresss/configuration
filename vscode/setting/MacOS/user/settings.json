{
    "editor.fontSize": 14,
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 14,
    "editor.renderWhitespace": "all",
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDragAndDrop": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "Visual Studio Light - C++",
    "files.autoSaveDelay": 1001,
    "cmake.configureOnOpen": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "go.inferGopath": false,
		"go.gopath": "${workspaceFolder}",
		"go.toolsGopath": "${env:GOPATH}",
		"go.useLanguageServer": false,
    "go.formatTool": "goimports",
    "python.pythonPath": "/usr/local/bin/python3",
		"python.formatting.provider": "yapf",
		"python.linting.enabled": true,
		"python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "C_Cpp.default.compilerPath": "/usr/local/bin/gcc-7",
		"C_Cpp.default.macFrameworkPath": [],
		"C_Cpp.default.intelliSenseMode": "gcc-x64",
		"C_Cpp.default.cStandard": "c11",
		"C_Cpp.default.cppStandard": "c++14",
		"C_Cpp.default.defines": [],
		"C_Cpp.default.includePath": [
			"${workspaceFolder}/**"
		],
		"C_Cpp.errorSquiggles": "Enabled",
		"C_Cpp.enhancedColorization": "Enabled",
		"C_Cpp.dimInactiveRegions": true,
		"C_Cpp.inactiveRegionOpacity": 0.8,
		"C_Cpp.inactiveRegionBackgroundColor": "#E0E0E0"
  }
  